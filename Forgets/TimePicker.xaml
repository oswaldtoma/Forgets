        <TextBox x:Name="SecTextBox" HorizontalAlignment="Left" Height="22" Margin="85,10,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="25"/>
        <Label Content=":" HorizontalAlignment="Left" Margin="34,6,0,0" VerticalAlignment="Top" Height="32" RenderTransformOrigin="0.513,0.938"/>
        <Label Content=":" HorizontalAlignment="Left" Margin="72,6,0,0" VerticalAlignment="Top" Height="32" RenderTransformOrigin="0.513,0.938"/>
        <Button Content="+" HorizontalAlignment="Left" Margin="115,6,0,0" VerticalAlignment="Top" Width="29" Height="15" RenderTransformOrigin="0.123,0.042"/>
